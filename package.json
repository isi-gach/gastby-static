{
  "name": "gatsby-static",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@wardpeet/gatsby-plugin-static-site": "^0.1.3",
    "gatsby": "^2.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "start": "gatsby develop",
    "prebuild": "rm -rf public web/public",
    "build": "cross-env PUBLIC_URL=/public gatsby build --prefix-paths",
    "postbuild": "mv public web && serve"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "gatsby-plugin-sharp": "^2.3.0",
    "gatsby-transformer-sharp": "^2.3.5",
    "prettier": "^1.19.1",
    "serve": "^11.2.0"
  }
}
